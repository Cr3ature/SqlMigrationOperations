<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RepositoryUrl>https://github.com/Cr3ature/SqlMigrationOperations</RepositoryUrl>
    <Authors>David Vanderheyden</Authors>
    <Product>SQL Migration Operations</Product>
    <PackageId>SqlMigrationOperations</PackageId>
    <Description>Custom Migration Operation SQL Generator for extending the standard migration operation in EntityFrameworkCore.SqlServer</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.2.4" />
  </ItemGroup>

</Project>
